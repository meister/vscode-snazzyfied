    },
    {
      "name": "Comments",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#777777",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Brackets",
      "scope": "meta.brace.round.begin, meta.brace.round.end, meta.brace.square.begin, meta.brace.square.end, meta.group.braces.round.function.arguments",
      "settings": {
        "foreground": "#5C6370"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Embedded Expression",
      "scope": "meta.embedded.expression",
      "settings": {
        "foreground": "#E05252"
      }
    },
    {
      "name": "Regex String",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#bbfcd1"
      }
    },
    {
      "name": "String URLs",
      "scope": "string.detected-link",
      "settings": {
        "foreground": "#bbfcd1"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "Constants",
      "scope": "constant.language",
      "settings": {
        "foreground": "#ff9f43"
      }
    },
    {
      "name": "User-defined constant",
